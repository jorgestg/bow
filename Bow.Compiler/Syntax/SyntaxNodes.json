{
  "definitions": {
    "CompilationUnit": {
      "children": {
        "ModClause": { "type": "ModClause", "optional": true },
        "UseClauses": { "type": "UseClause", "array": true },
        "Items": { "type": "Item", "array": true }
      }
    },
    "ModClause": {
      "children": {
        "ModKeyword": "Token",
        "Name": "IdentifierToken"
      }
    },
    "UseClause": {
      "children": {
        "UseKeyword": "Token",
        "Name": "Name"
      }
    },
    "Name": { "abstract": true },
    "SimpleName": {
      "extends": "Name",
      "children": {
        "Identifier": "IdentifierToken"
      }
    },
    "QualifiedName": {
      "extends": "Name",
      "children": {
        "Parts": { "type": "IdentifierToken", "array": true }
      }
    },
    "TypeReference": {
      "abstract": true
    },
    "NamedTypeReference": {
      "extends": "TypeReference",
      "children": {
        "Name": "Name"
      }
    },
    "KeywordTypeReference": {
      "extends": "TypeReference",
      "children": {
        "Keyword": "Token"
      }
    },
    "PointerTypeReference": {
      "extends": "TypeReference",
      "children": {
        "Star": "Token",
        "Type": "TypeReference"
      }
    },
    "Item": {
      "abstract": true,
      "children": {
        "AccessModifier": { "type": "Token", "optional": true },
        "Identifier": "IdentifierToken"
      }
    },
    "StructDefinition": {
      "extends": "Item",
      "children": {
        "Slot": "int",
        "AccessModifier": {
          "type": "Token",
          "optional": true,
          "override": true
        },
        "Keyword": "Token",
        "Identifier": { "type": "IdentifierToken", "override": true },
        "OpenBrace": "Token",
        "Fields": { "type": "FieldDeclaration", "array": true },
        "Methods": { "type": "FunctionDefinition", "array": true },
        "CloseBrace": "Token"
      }
    },
    "FieldDeclaration": {
      "children": {
        "Slot": "int",
        "AccessModifier": { "type": "Token", "optional": true },
        "MutKeyword": { "type": "Token", "optional": true },
        "Identifier": "IdentifierToken",
        "Type": "TypeReference"
      }
    },
    "EnumDefinition": {
      "extends": "Item",
      "children": {
        "Slot": "int",
        "AccessModifier": {
          "type": "Token",
          "optional": true,
          "override": true
        },
        "Keyword": "Token",
        "Identifier": { "type": "IdentifierToken", "override": true },
        "OpenBrace": "Token",
        "Cases": { "type": "EnumCaseDeclaration", "array": true },
        "Methods": { "type": "FunctionDefinition", "array": true },
        "CloseBrace": "Token"
      }
    },
    "EnumCaseDeclaration": {
      "children": {
        "Slot": "int",
        "Identifier": "IdentifierToken",
        "Argument": {
          "type": "EnumCaseArgument",
          "optional": true
        }
      }
    },
    "EnumCaseArgument": {
      "children": {
        "OpenParenthesis": "Token",
        "TypeReference": "TypeReference",
        "CloseParenthesis": "Token"
      }
    },
    "FunctionDefinition": {
      "extends": "Item",
      "children": {
        "Slot": "int",
        "AccessModifier": {
          "type": "Token",
          "optional": true,
          "override": true
        },
        "Keyword": "Token",
        "Identifier": { "type": "IdentifierToken", "override": true },
        "OpenParenthesis": "Token",
        "Parameters": { "type": "ParameterDeclaration", "array": true },
        "CloseParenthesis": "Token",
        "ReturnType": { "type": "TypeReference", "optional": true },
        "Body": "BlockStatement"
      }
    },
    "ParameterDeclaration": {
      "abstract": true,
      "children": {
        "MutKeyword": { "type": "Token", "optional": true }
      }
    },
    "SimpleParameterDeclaration": {
      "extends": "ParameterDeclaration",
      "children": {
        "MutKeyword": { "type": "Token", "optional": true, "override": true },
        "Identifier": "IdentifierToken",
        "Type": "TypeReference"
      }
    },
    "SelfParameterDeclaration": {
      "extends": "ParameterDeclaration",
      "children": {
        "MutKeyword": { "type": "Token", "optional": true, "override": true },
        "Star": { "type": "Token", "optional": true },
        "Keyword": "Token",
        "Type": { "type": "TypeReference", "optional": true }
      }
    },
    "Initializer": {
      "children": {
        "Operator": "Token",
        "Expression": "Expression"
      }
    },
    "Statement": {
      "abstract": true
    },
    "LocalDeclaration": {
      "extends": "Statement",
      "children": {
        "LetKeyword": "Token",
        "MutKeyword": { "type": "Token", "optional": true },
        "Identifier": "IdentifierToken",
        "Type": { "type": "TypeReference", "optional": true },
        "Initializer": { "type": "Initializer", "optional": true }
      }
    },
    "BlockStatement": {
      "extends": "Statement",
      "children": {
        "OpenBrace": "Token",
        "Statements": { "type": "Statement", "array": true },
        "CloseBrace": "Token"
      }
    },
    "ExpressionStatement": {
      "extends": "Statement",
      "children": {
        "Expression": "Expression"
      }
    },
    "ReturnStatement": {
      "extends": "Statement",
      "children": {
        "Keyword": "Token",
        "Expression": { "type": "Expression", "optional": true }
      }
    },
    "IfStatement": {
      "extends": "Statement",
      "children": {
        "Keyword": "Token",
        "Condition": "Expression",
        "Then": "BlockStatement",
        "ElseIfs": { "type": "ElseIfClause", "array": true },
        "Else": { "type": "ElseClause", "optional": true }
      }
    },
    "ElseIfClause": {
      "children": {
        "ElseKeyword": "Token",
        "IfKeyword": "Token",
        "Condition": "Expression",
        "Body": "BlockStatement"
      }
    },
    "ElseClause": {
      "children": {
        "ElseKeyword": "Token",
        "Body": "BlockStatement"
      }
    },
    "WhileStatement": {
      "extends": "Statement",
      "children": {
        "Keyword": "Token",
        "Condition": "Expression",
        "Body": "BlockStatement"
      }
    },
    "AssignmentStatement": {
      "extends": "Statement",
      "children": {
        "Assignee": "Expression",
        "Operator": "Token",
        "Expression": "Expression"
      }
    },
    "Expression": {
      "abstract": true
    },
    "LiteralExpression": {
      "extends": "Expression",
      "children": {
        "Literal": "Token"
      }
    },
    "ParenthesizedExpression": {
      "extends": "Expression",
      "children": {
        "OpenParenthesis": "Token",
        "Expression": "Expression",
        "CloseParenthesis": "Token"
      }
    },
    "IdentifierExpression": {
      "extends": "Expression",
      "children": {
        "Identifier": "IdentifierToken"
      }
    },
    "CallExpression": {
      "extends": "Expression",
      "children": {
        "Callee": "Expression",
        "OpenParenthesis": "Token",
        "Arguments": { "type": "Expression", "array": true },
        "CloseParenthesis": "Token"
      }
    },
    "UnaryExpression": {
      "extends": "Expression",
      "children": {
        "Operator": "Token",
        "Operand": "Expression"
      }
    },
    "BinaryExpression": {
      "extends": "Expression",
      "children": {
        "Left": "Expression",
        "Operator": "Token",
        "Right": "Expression"
      }
    },
    "StructCreationExpression": {
      "extends": "Expression",
      "children": {
        "Dot": "Token",
        "OpenBrace": "Token",
        "FieldInitializers": {
          "type": "StructCreationFieldInitializer",
          "array": true
        },
        "CloseBrace": "Token"
      }
    },
    "StructCreationFieldInitializer": {
      "children": {
        "Identifier": "IdentifierToken",
        "Initializer": { "type": "Initializer", "optional": true }
      }
    }
  }
}
